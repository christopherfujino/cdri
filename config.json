{
  "owner": "Christopher Fujino",
  "dotfiles" : [
    {
      "name" : ".bash_profile",
      "target" : "~/.bash_profile",
      "platform" : "linux"
    },
    {
      "name" : ".bashrc",
      "target" : "~/.bashrc",
      "platform" : "linux"
    },
    {
      "name" : ".gitconfig",
      "target" : "~/.gitconfig",
      "platform" : "any"
    },
    {
      "name" : "i3config",
      "target" : "~/.i3/config",
      "platform" : "linux"
    },
    {
      "name" : ".npmrc",
      "target" : "~/.npmrc",
